package exercise.overall.processor;

public class ProcessTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Processor[] procs = new Processor[] {
			new LowerCase(),
			new UpperCase(),
			new Splitter()
		};
		
		
		for( Processor proc : procs) {
			System.out.println("Processor 명 " + proc.name());
			System.out.println("결과 출력 : " + proc.process("I'm so sorry but I love you. 다 거짓말~~"));
			System.out.println();
		}
	}

}
