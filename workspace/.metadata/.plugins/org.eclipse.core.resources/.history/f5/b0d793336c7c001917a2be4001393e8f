package exercise.overall.hero;

public abstract class Hero implements CanFight, CanFly, CanSwim {

	@Override
	public void swim() {
		System.out.println(" 헤엄친다.");

	}

	@Override
	public void fly() {
		System.out.println(" 난다.");

	}

	@Override
	public void fight() {
		System.out.println(" 헤엄친다.");

	}
	
	abstract void action();

}
