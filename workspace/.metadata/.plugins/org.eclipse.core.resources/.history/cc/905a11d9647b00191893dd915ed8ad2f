package lab.java.core;

class Parent {
	
	String name = "parent";
	
	public void work() {
		System.out.println("IT");
	}
}

class Child extends Parent {
	
}

public class ExtendsTest1 {
	public static void main(String[] args) {
		Child c1 = new Child();
		System.out.println(c1.name);
		c1.name = "child"
	}
}