package exercise.overall.processor;

public class ProcessTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Processor[] procs = new Processor[] {
			new LowerCase(),
			new UpperCase(),
			new Splitter()
		};
		
		
		for( Processor proc : procs) {
			proc.name();
			proc.process("I'm so sorry but I love you. 다 거짓말~~");
		}
	}

}
