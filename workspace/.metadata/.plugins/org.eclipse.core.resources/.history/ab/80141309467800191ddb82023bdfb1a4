package excerics.class2;

public class Student {
	private String studentId;
	private int c;
	private int linux;
	private int java;
	
	public Student() {};
	public Student(String studentId, int c, int linux, int java) {
		this.studentId = studentId;
		this.c = c;
		this.linux = linux;
		this.java = java;
	}
	
	public String getStudentId() {	
		return studentId;	
	}
	
	public void setStudentId(String studentId) {
		this.studentId = studentId;	
	}
	
	public int getC() {	
		return c;	
	}
	
	public void setC(int c) {	
		this.c = c;	
	}
	
	public int getLinux() {	
		return linux;	
	}
	
	public void setLinux(int linux) {	
		this.linux = linux;	
	}
	
	public int getJava() {	
		return java;	
	}
	
	public void sestJava(int java) {	
		this.java = java;	
	}
	
	public int getTotal() {
		return c + linux + java;
	}
	
	@Override
	public String toString() {
		return String.format("C: %d점, Linux: %d점, Java: %d점\n", c,	linux, java);
	}
	
	
}
