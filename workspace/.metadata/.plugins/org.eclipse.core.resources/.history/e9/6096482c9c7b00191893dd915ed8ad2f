package exercise.inheritance.Language;

public class LanguageTest {

	public static void main(String[] args) {
		Language l1 = new Korean();
		Language l2 = new English();
		Language l3 = new Japanese();
		
		l1.sayHello();
		l2.sayHello();
		l3.sayHello();
		
		System.out.println(l1.region);
		System.out.println(l2.region);
		System.out.println(l3.region);
		
		l1.printRegion();
		l1.printRegion();
		l1.printRegion();
	}

}
